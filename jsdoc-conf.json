{
    "source": {
        "include": ["src", "README.md"],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "name": "Dom"
    },
    "opts": {
        "recurse": true,
        "destination": "doc",
        "template": "node_modules/tui-jsdoc-template",
        "package": "package.json"
    }
}
